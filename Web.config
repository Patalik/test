<configuration>
	
    <!--–––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––-->
	<!-- Nastavení aplikace -->
	<!--–––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––––-->
	<appSettings>
		<add key="emailWebmaster" value="info@travelsoft.cz"/>
	</appSettings>
		
	<system.web>
		<customErrors mode="Off"/>
		<compilation debug="true" strict="false" explicit="true" targetFramework="4.5" >
			<codeSubDirectories>
				<add directoryName="CS"/>
				<add directoryName="VB"/>
			</codeSubDirectories>			
		</compilation>
		<pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" />
		<globalization culture="cs-CZ" uiCulture="cs-CZ" requestEncoding="utf-8" responseEncoding="utf-8" />
		<authentication mode="Windows"/>
		<httpRuntime maxRequestLength="16392" executionTimeout="45" />				
	</system.web>	

</configuration>
